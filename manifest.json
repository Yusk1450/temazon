{
	"name": "Temazon"
	,"version": "0.1"
	,"description": "Temazon"
	,"manifest_version": 2

	,"icons": {
		"16": "icon16.png"
		,"48": "icon48.png"
		,"128": "icon128.png"
	}
	,"permissions": [
		"tabs", "http://*/*", "https://*/*"
	]
	,"content_scripts": [
		{
			"matches": ["http://www.amazon.co.jp/*"]
			,"css": ["style.css"]
			,"js": [
				"jquery-2.1.3.min.js"
				,"floating_window.js"
				,"content_script.js"
			]
			,"run_at": "document_end"
		}
	]

}